.all-protocols {
  font-size: 24px;
  color: #666;
  p,div {
    line-height: 150%;
  }
  p {
    margin: 10px 0;
    text-indent: 48px;
  }
  &.protocol-wrap {
    padding:0 30px;
  }
  .part {
    padding: 40px 0;
  }
  .level-one {
    line-height: 150%;
    font-size: 40px;
    color: #333;
    text-align: center;
    padding: 32px 0;
  }
  .level-two {
    line-height: 150%;
    font-size: 32px;
    color: #333;
    padding: 24px 0;
  }
  .level-thr {
    line-height: 150%;
    font-size: 28px;
    color: #333;
    padding: 24px 0;
  }
  //借款协议
  .protocol-form {
    line-height: 150%;
    .protocol-tr {
      text-align: center;
      display: flex;
      display: -webkit-flex;
      -webkit-box-align:center;
      border: 2px solid #b2b2b2;
      &:not(:last-child ){
        border-bottom: none;
      }
      .protocol-td {
        flex: 1;
        -webkit-flex: 1;
        padding: 12px 0;
        &:not(:last-child) {
          border-right: 1px solid #b2b2b2;
        }
      }
      .protocol-td-text {
        padding:40px;
        text-align: left;
        color: #666;
      }
    }
  }
}
